SELECT A.MEMBER_NAME, B.REVIEW_TEXT, DATE_FORMAT(B.REVIEW_DATE, "%Y-%m-%d") as REVIEW_DATE FROM (SELECT A.MEMBER_ID, A.MEMBER_NAME,RANK() OVER(ORDER BY CNT DESC) as Ranks FROM (SELECT A.MEMBER_ID, A.MEMBER_NAME ,COUNT(A.MEMBER_ID) as CNT FROM MEMBER_PROFILE as A JOIN REST_REVIEW as B ON A.MEMBER_ID = B.MEMBER_ID GROUP BY A.MEMBER_ID) as A) A JOIN REST_REVIEW B ON A.MEMBER_ID = B.MEMBER_ID WHERE A.Ranks  = 1 ORDER BY REVIEW_DATE